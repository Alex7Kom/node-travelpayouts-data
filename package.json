{
  "name": "travelpayouts-data",
  "version": "2.0.0",
  "description": "Travelpayouts (Aviasales) data files with query interface",
  "main": "src/index.js",
  "keywords": [
    "travelpayouts",
    "aviasales",
    "API"
  ],
  "scripts": {
    "lint": "eslint \"src/**/*.{j,t}s?(x)\"",
    "pretty": "pretty-quick",
    "test": "jest"
  },
  "author": "Alexey Komarov <alex7kom@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alex7Kom/node-travelpayouts-data.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "find-cache-dir": "^3.3.1",
    "json-query": "^2.2.2",
    "mkdirp": "^1.0.4"
  },
  "devDependencies": {
    "@alex7kom/sayonara-config": "^0.5.0",
    "husky": "^3.0.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/@alex7kom/sayonara-config/src/configs/eslint-node.js",
    "overrides": [
      {
        "files": [
          "*.?(test|spec).{j,t}s?(x)"
        ],
        "env": {
          "jest": true,
          "node": true,
          "es6": true
        },
        "parserOptions": {
          "ecmaVersion": 2018
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "sayonaraConfig": {
    "sets": [
      "node-library",
      "npm-package"
    ]
  }
}
